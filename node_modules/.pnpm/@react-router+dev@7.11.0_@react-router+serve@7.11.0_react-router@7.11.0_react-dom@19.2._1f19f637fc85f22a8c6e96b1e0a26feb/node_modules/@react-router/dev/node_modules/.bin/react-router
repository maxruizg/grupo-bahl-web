#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxruizg/Workspace/grupo-bahlcasa/node_modules/.pnpm/@react-router+dev@7.11.0_@react-router+serve@7.11.0_react-router@7.11.0_react-dom@19.2._1f19f637fc85f22a8c6e96b1e0a26feb/node_modules/@react-router/dev/node_modules:/Users/maxruizg/Workspace/grupo-bahlcasa/node_modules/.pnpm/@react-router+dev@7.11.0_@react-router+serve@7.11.0_react-router@7.11.0_react-dom@19.2._1f19f637fc85f22a8c6e96b1e0a26feb/node_modules/@react-router/node_modules:/Users/maxruizg/Workspace/grupo-bahlcasa/node_modules/.pnpm/@react-router+dev@7.11.0_@react-router+serve@7.11.0_react-router@7.11.0_react-dom@19.2._1f19f637fc85f22a8c6e96b1e0a26feb/node_modules:/Users/maxruizg/Workspace/grupo-bahlcasa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxruizg/Workspace/grupo-bahlcasa/node_modules/.pnpm/@react-router+dev@7.11.0_@react-router+serve@7.11.0_react-router@7.11.0_react-dom@19.2._1f19f637fc85f22a8c6e96b1e0a26feb/node_modules/@react-router/dev/node_modules:/Users/maxruizg/Workspace/grupo-bahlcasa/node_modules/.pnpm/@react-router+dev@7.11.0_@react-router+serve@7.11.0_react-router@7.11.0_react-dom@19.2._1f19f637fc85f22a8c6e96b1e0a26feb/node_modules/@react-router/node_modules:/Users/maxruizg/Workspace/grupo-bahlcasa/node_modules/.pnpm/@react-router+dev@7.11.0_@react-router+serve@7.11.0_react-router@7.11.0_react-dom@19.2._1f19f637fc85f22a8c6e96b1e0a26feb/node_modules:/Users/maxruizg/Workspace/grupo-bahlcasa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
